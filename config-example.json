{
  "general": {
    "program_name": "mattermost-newsfeeds",
    "user_agent": "mattermost-newsfeeds/3.0 (mailto:XXXX@YYYY.ZZZ)",
    "dry_run": false,
    "seen_store_path": "state/seen.json",
    "seen_ttl_days": 7,
    "location": {
      "lat": LATITUDE,
      "lon": LONGITUDE_WEST_IS_NEGATIVE
    },
    "timezone": "America/Los_Angeles",
    "log_level": "INFO",
    "log_time_format": "%H:%M:%S %a %b %d, %Y",
    "sleep_min": 1,
    "sleep_max": 5,
    "mattermost": {
      "host": "AAAAA.BBBBB.CCC",
      "token": "xxxxxxxxxxxxxxxxxxxxxx",
      "scheme": "http",
      "port": 80,
      "basepath": "/api/v4",
      "team": "TTTTTTTTTTTTTTT",
      "user": "UUUUUUUU"
    }
  },
  "sources": [
    {
      "name": "NWS",
      "enabled": true,
      "module": "sources.nws",
      "class": "NWS",
      "poll_seconds": 60,
      "params": {
        "api_url": "https://api.weather.gov/alerts/active"
      },
      "template": "**[NWS] {event}** \u2014 {headline}\n_effective:_ {effective}  _expires:_ {expires}",
      "notifier": {
        "type": "mattermost",
        "stream": true,
        "style": "markdown",
        "webhook_url": "",
        "channel": "National Weather Service"
      }
    },
    {
      "name": "USGS",
      "enabled": true,
      "module": "sources.usgs",
      "class": "USGS",
      "poll_seconds": 60,
      "params": {
        "feed_url": "https://earthquake.usgs.gov/earthquakes/feed/v1.0/summary/all_day.geojson",
        "max_mi": 100.0
      },
      "template": "**[USGS]** Earthquake M{mag} at {timestamp_local} \u2014 {place} ({distance_mi_from_origin} mi)\n{url}",
      "notifier": {
        "type": "mattermost",
        "stream": true,
        "style": "markdown",
        "webhook_url": "",
        "channel": "US Geological Survey"
      }
    },
    {
      "name": "CalTrans",
      "enabled": true,
      "module": "sources.caltrans",
      "class": "Caltrans",
      "poll_seconds": 60,
      "params": {
        "max_mi": 10.0,
        "endpoints": {
          "lane_closures": "https://quickmap.dot.ca.gov/data/lcs2way.kml",
          "chp_incidents": "https://quickmap.dot.ca.gov/data/chp-only.kml",
          "message_signs": "https://quickmap.dot.ca.gov/data/cms.kml",
          "waze_road_closed": "https://quickmap.dot.ca.gov/data/waze_road_closed.kml"
        },
        "layer_filter_prefix": ""
      },
      "template": "**[CalTrans]** \u2014 {name} at {timestamp_local} ({distance_mi} mi)\n{desc}",
      "notifier": {
        "type": "mattermost",
        "stream": true,
        "style": "markdown",
        "webhook_url": "",
        "channel": "CalTrans"
      },
      "acronyms": {
        "ABV": "above",
