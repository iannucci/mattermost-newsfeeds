    {
      "name": "PAO",
      "enabled": false,
      "module": "sources.pao",
      "class": "PAO",
      "poll_seconds": 600,
      "params": {
        "news_url": "https://www.paloaltoonline.com/news/",
        "max_items": 15
      },
      "template": "**[PAO]** \u2014 {title}\n{link}",
      "notifier": {
        "type": "mattermost",
        "stream": true,
        "style": "markdown",
        "webhook_url": "",
        "channel": "Palo Alto Online"
      }
    },
    {
      "name": "PulsePoint",
      "enabled": false,
      "module": "sources.pulsepoint",
      "class": "PulsePoint",
      "poll_seconds": 60,
      "params": {
        "agency_id": "43060",
        "respond_url": "https://web.pulsepoint.org/?agencies=43060",
        "broadcastify": "https://www.broadcastify.com/listen/feed/34259"
      },
      "template": "**PulsePoint** \u2014 Palo Alto FD\nRespond: {respond_url}\nRadio: {broadcastify}",
      "notifier": {
        "type": "mattermost",
        "stream": true,
        "style": "markdown",
        "webhook_url": "",
        "channel": "PulsePoint"
      }
    },
    {
      "name": "AmbientWeather",
      "enabled": true,
      "module": "sources.ambient_weather",
      "class": "AmbientWeather",
      "poll_seconds": 600,
      "mode": "http",
      "params": {
        "http": {
          "host": "0.0.0.0",
          "port": 46000,
          "pretty": true,
          "verbose": false,
          "dump_on_empty": false
        },
        "udp": {
          "iface": "en0",
          "port": 59387,
          "pretty": true,
          "dump_on_empty": true
        }
      },
      "template": "**[AmbientWeather]** \u2014 Local conditions at {timestamp_local}:\n  Temp {temperature_F}\u00b0F\n  Winds from the {wind_dir} at {wind_mph}, gusting to {wind_gust_mph} mph\n  Humidity {humidity_pct}%\n  Pressure {pressure_in_hg} in Hg\n  UV index {uv_index}\n  Rain rate {rain_rate_in_hr} in/h, daily {rain_daily_in} in",
      "notifier": {
        "type": "mattermost",
        "stream": true,
        "style": "markdown",
        "webhook_url": "",
        "channel": "Local Weather"
      }
    }
  ]
